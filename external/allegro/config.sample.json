{
	"scrapeUrl": "https://allegro.pl.allegrosandbox.pl/kategoria/komputery?order=n",
	"xpath": "//section//article//h2/a",
	"delay": 600,
	"delayRand": 60,
	"chromeSettings": {
		"executablePath": "google-chrome-stable",
		"args": [
			"--no-sandbox",
			"--disable-dev-shm-usage"
		],
		"headless": true,
		"defaultViewport": {
			"width": 1920,
			"height": 1080
		}
	},
	"redis": {
		"host": "cebula-redis:6379",
		"cache_key": "cebulapy_external_cache",
		"data_key": "cebulapy_external"
	}
}
